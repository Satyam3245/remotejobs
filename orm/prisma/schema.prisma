generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Jobs{
  id String @default(uuid()) @unique @id
  company_name String
  employment_type String @default("")
  Domain String @default("")
  role String
  location String
  salary Int @default(5000)
  createdAt DateTime @default(now())
  skill String[]
}